	'use strict';
	var _ = __webpack_require__(388);

	/**
	 * Choice object
	 * Normalize input as choice object
	 * @constructor
	 * @param {String|Object} val  Choice value. If an object is passed, it should contains
	 *                             at least one of `value` or `name` property
