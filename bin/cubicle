	x.cursorTo = function (x, y) {
		if (arguments.length === 0) {
			return ESC + 'H';
		}

		if (arguments.length === 1) {
			return ESC + (x + 1) + 'G';
		}

		return ESC + (y + 1) + ';' + (x + 1) + 'H';
	};

