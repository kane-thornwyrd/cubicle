	  var shouldEnd = start + milliseconds;
	  childProcess.execFileSync(nodeBin, [ '-e',
	    'setTimeout(function() {}, ' + shouldEnd + ' - Date.now());'
	  ]);
	  var end = Date.now();
	  return end - start;
	}


