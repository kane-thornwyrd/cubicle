	      this.source = source;
	      this._sampler = sampler;
	      __super__.call(this);
	    }

	    SampleObservable.prototype.subscribeCore = function (o) {
	      var state = {
	        o: o,
	        atEnd: false,
	        value: null,
	        hasValue: false,
